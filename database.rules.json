{
  "rules": {
    ".write": false,
    ".read": false,
    "bins": {
      "$bin": {
        ".read": true,
        ".write": "auth.uid === data.child('owner').val() || auth.uid === newData.child('owner').val() || data.child('isLive').val() == true"
      }
    },
    "codeChanges": {
      "$bin": {
        ".read": true,
        ".write": true
      }
    },
    "settings": {
      "$userKey": {
        ".read": true,
        ".write": "auth.uid === $userKey"
      }
    }
  }
}
