{
  "name": "ducky-web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "6.9.4",
    "npm": "3.10.8"
  },
  "scripts": {
    "script": "node scripts/index",
    "heroku-postbuild": "npm run build:client && npm run build:dashboard && npm run deploy",
    "service": "cross-env DUCKY_ENV=ducky-dev node service/updateFirebase/index && cross-env DUCKY_ENV=ducky-dev nodemon service/index.js",
    "client": "cross-env DUCKY_ENV=ducky-dev webpack-dev-server --content-base public/",
    "dashboard": "cross-env DUCKY_ENV=ducky-dev webpack-dev-server --config webpack.dashboard.config.js --content-base publicDashboard/",
    "build:dashboard": "webpack --config webpack.dashboard.config.js",
    "build:client": "webpack",
    "deploy": "node service/updateFirebase/index && firebase deploy --project $DUCKY_ENV --public public --only \"hosting,database\"",
    "deploy:rules": "cross-env DUCKY_ENV=ducky-dev node service/updateFirebase/index && firebase deploy --project ducky-test --public public --only \"database\"",
    "test": "npm run test:unit && npm run test:integration && npm run test:flow",
    "test:unit": "npm run test:taskHandlers:unit && npm run test:utils:unit",
    "test:integration": "npm run test:taskHandlers:integration && npm run test:calculator:integration",
    "test:flow": "npm run test:taskHandlers:flow",
    "test:taskHandlers:unit": "mocha \"tests/taskHandlers/unit/**/*.js\"",
    "test:taskHandlers:integration": "mocha \"tests/taskHandlers/integration/**/*.js\"",
    "test:taskHandlers:flow": "mocha \"tests/taskHandlers/flow/**/*.js\"",
    "test:calculator:integration": "mocha tests/calculator/integration",
    "test:utils:unit": "mocha \"tests/utils/unit/**/*.js\""
  },
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.16.0",
    "body-parser": "^1.15.2",
    "cerebral": "2.0.0-b-alpha.95daa363",
    "cerebral-forms": "1.0.0-b-alpha.757a81fc",
    "cerebral-module-useragent": "1.0.0-b-alpha.95daa363",
    "cerebral-provider-firebase": "1.0.0-b-alpha.757a81fc",
    "cerebral-provider-http": "1.0.0-b-alpha.757a81fc",
    "cerebral-router": "1.0.0-b-alpha.757a81fc",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "classnames": "^2.2.5",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "ducky-components": "git+https://github.com/DuckyTeam/ducky-components.git#8739c90",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "faye-websocket": "^0.11.0",
    "file-loader": "^0.9.0",
    "firebase": "^3.6.2",
    "firebase-admin": "^4.0.4",
    "firebase-queue": "^1.6.0",
    "firebase-tools": "^3.0.0",
    "function-tree": "0.5.0-b-alpha.95daa363",
    "gcloud": "^0.37.0",
    "grpc": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "imagemagick-stream": "^4.1.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "json-loader": "^0.5.4",
    "mailchimp-api-v3": "^1.6.0",
    "mandrill-api": "^1.0.45",
    "markdown-to-react-components": "^0.2.2",
    "md5": "^2.2.1",
    "mixpanel-browser": "^2.8.1",
    "mocha": "^3.0.2",
    "moment": "^2.14.1",
    "multiparty": "^4.1.2",
    "node-polyglot": "^2.2.2",
    "nodeunit": "^0.9.1",
    "proxyquire": "^1.7.10",
    "react": "^15.4.1",
    "react-date-picker": "^5.3.26",
    "react-dom": "15.4.1",
    "react-masonry-component": "^4.0.4",
    "react-tappable": "^0.8.4",
    "react-textarea-autosize": "^4.0.2",
    "socket.io": "^1.4.8",
    "socket.io-client": "^1.4.8",
    "style-loader": "^0.13.1",
    "svgo-loader": "^1.1.0",
    "upload-controller": "^0.2.6",
    "url-loader": "^0.5.7",
    "username": "^2.2.2",
    "uuid": "^2.0.2",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "cross-env": "^2.0.1",
    "eslint": "^2.11.0",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "firebase-server": "^0.7.1",
    "install": "^0.8.4",
    "minimist": "^1.2.0",
    "nodemon": "^1.9.2",
    "webpack-dev-server": "1.14.1",
    "yargs": "^6.6.0"
  }
}
